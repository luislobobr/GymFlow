{
    "version": 2,
    "buildCommand": null,
    "outputDirectory": ".",
    "framework": null,
    "installCommand": null,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin-allow-popups"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "credentialless"
                }
            ]
        },
        {
            "source": "/js/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, must-revalidate"
                }
            ]
        }
    ]
}